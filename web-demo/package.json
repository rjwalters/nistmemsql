{
  "name": "nistmemsql-web-demo",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,css}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky",
    "debug:site": "npx tsx debug-credentials-error.ts",
    "inspect:site": "npx tsx inspect-site.ts",
    "inspect:firefox": "npx tsx inspect-firefox.ts",
    "find:credentials": "npx tsx find-credentials-usage.ts",
    "test:execution": "npx tsx test-execution.ts",
    "diagnose:data": "npx tsx diagnose-data-format.ts",
    "test:local": "npx tsx test-local.ts"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@playwright/test": "^1.56.1",
    "@vitest/coverage-v8": "^4.0.3",
    "@vitest/ui": "^4.0.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "happy-dom": "^20.0.8",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "lint-staged": "^16.2.6",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.46.2",
    "vite": "^5.0.0",
    "vite-plugin-wasm": "^3.3.0",
    "vitest": "^4.0.3"
  }
}
